  <%if(typeof message!='undefined' ){%>
      <input type="hidden" class="message" value="<%=message.message+ ', Error : '+ code%>">
      <%if(typeof success!='undefined' ){%>
        <script>
          var message = document.querySelector('.message').value
          var Toast = Swal.mixin({
            showConfirmButton: false,
            background: "green",
            timerProgressBar: true,
            color:"white"
          })
          Toast.fire({
            title: message,
            icon: "success"
          })
        </script>
        <%}else if(typeof warning!='undefined' ){%>
          <script>
            var message = document.querySelector('.message').value
            var Toast = Swal.mixin({
              showConfirmButton: false,
              timerProgressBar: true,
            })
            Toast.fire({
              title: message,
              icon: "warning"
            })
          </script>
          <%}else{%>
            <script>
              var message = document.querySelector('.message').value
              var Toast = Swal.mixin({
                showConfirmButton: false,
                timerProgressBar: true,
                color: "black",
                returnFocus:false,
                allowOutsideClick:false,
              })
              Toast.fire({
                title: message,
                icon: "error"
              })
              console.log(document.querySelector('.swal2-shown').getAttribute('class'));
            </script>
            <%}%>
              <%}%>
              <style>
                .swal2-shown{
                  background: red;
                }
              </style>
  